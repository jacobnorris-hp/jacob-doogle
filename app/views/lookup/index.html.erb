<h1>Doogle</h1>
<br/>

<%= form_for(:entry, :remote => true) do |f| %>
  <div class="field">
    <%= f.label :word %><br>
    <%= f.text_field :word %>
  </div>
  <div class="field">
    <%= f.label :definition %><br>
    <%= f.text_field :definition %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
